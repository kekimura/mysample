      <method siena:requiresAuthentication="true" name="POST" id="AddMemberToGroup" actionName="AddMemberToGroup" visibility="important" siena:isDeprecated="false">
        <doc title="グループへのメンバーの追加&#xA;この操作は、UPN を使用して、O365 グループにメンバーを追加するために使用されます。" />
        <request>
          <param style="query" name="userUpn" type="xs:string" required="true">
            <doc title="ユーザーのユーザー プリンシパル名 (UPN)。" />
          </param>
        </request>
      </method>
    </resource>
