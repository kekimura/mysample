  <resources siena:authenticationProviderHref="#PowerAppAuth" base="https://japan-001.azure-apim.net/apim/office365groups">
    <resource path="/{connectionId}/v1.0/groups/{groupId}/members">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <param style="template" name="groupId" type="xs:string" required="true">
        <doc title="ドロップダウンからグループを選択するか、グループ ID を入力します。" />
      </param>
      <method siena:requiresAuthentication="true" name="GET" id="ListGroupMembers" actionName="ListGroupMembers" visibility="advanced" siena:isDeprecated="false">
        <doc title="グループ メンバーの一覧表示&#xA;この操作を実行すると、指定したグループ内のすべてのメンバーの一覧のほかに、名前、タイトル、電子メールなどの詳細情報が返されます。Top パラメーターを使用すると、1000 件までの項目をクエリできます。1000 件を超える値を取得する必要がある場合は、設定 -&gt; 改ページの機能を有効にして、しきい値の制限を指定してください。" />
        <request>
          <param style="query" name="$top" type="xs:int">
            <doc title="取得するグループ メンバーの数 (1 から 999、既定値は 100)。" />
          </param>
        </request>
        <response>
          <representation mediaType="application/json" element="service:ListGroupMembers_Response" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/v1.0/groups/{groupId}/members/$ref">
